<%= simple_form_for(book) do |f| %>
  <%= f.input :title %>
  <%= f.input :author %>
  <%= f.input :published_date %>
  <%= f.input :rating %>
  <%= f.input :publisher %>
  <%= f.input :category %>
  <%= f.input :photo, as: :file %>
  <%= f.button :submit, class:"btn btn-outline-success my-2 my-sm-0 mr-2" %>
<% end %>
